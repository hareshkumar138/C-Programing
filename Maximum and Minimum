#include <stdio.h>

int main()
{
  int a,b,c;
  printf("Enter three numbers: ");
  scanf("%d%d%d",&a,&b,&c);
  int d=a;
  if (b>d){
    d=b;
  }
  if (c>d){
    d=c;
  }
  int e=a;
  if (b<e){
    e=b;
  }
  if (c<e){
    e=c;
  }
  printf("Maximum number: %d\n", d);
  printf("Minimum number: %d\n", e);
  return 0;
}
