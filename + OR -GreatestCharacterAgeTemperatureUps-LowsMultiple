#include <stdio.h>
int main()
{
  int n;
  scanf("%d",&n);
  if ((n%3==0)&&(n%5==0))
  {
    printf("%d is multiple of 3 and 5",n);
  }
  else if (n%5==0)
  {
    printf("%d is a multiple of 5",n);
  }
  else if(n%3==0)
  {
   printf("%d is a multiple of 3",n); 
  }
  else
  {
    printf("%d is not a multiple of both 3 and 5",n);
  }
  return 0;
}
