#include<stdio.h>
int main()
{
  int a[2][2]={1,2,3,4},b[2][2]={1,2,3,4},count=0;
  int i,j;
  for(i=0;i<2;i++)
  {
    for(j=0;j<2;j++)
    {
      if (a[i][j]==b[i][j])
      {
        count+=1;
      }
    }
  }
  if (count==4)
  { 
  printf("matrix is equal");
  }
  else
  {
    printf("it is not equal");
  }
  return 0;
}
