#include<stdio.h>
#include<string.h>
struct person
{
  char name[50];
  int phno;
  char email[235];
};
int main()
{
  struct person p1,p2;
  strcpy(p1.name,"safsaf");
  scanf("%d",&p1.phno);
  strcpy(p1.email,"safajf2@gmail.com");
  printf("name:%s\nphone no:%d\nemail address:%s\n",p1.name,p1.phno,p1.email);
  
  strcpy(p2.name,"asfas");
  scanf("%d",&p2.phno);
  strcpy(p2.email,"afgagwh@gmail.com");
  printf("name:%s\nphone no:%d\nemail address:%s",p2.name,p2.phno,p2.email);
  return 0;
}
