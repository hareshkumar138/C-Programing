#include <stdio.h>
#include <string.h>
struct c
{
  int a,d,m,y;
};
int main()
{
  struct c c1,c2,c3;
  c1.a=35;
  c1.d=1;
  c1.m=1;
  c1.y=2025;
  
  c2.a=40;
  c2.d=1;
  c2.m=2;
  c2.y=2025;
  
  c3.a=20;
  c3.d=1;
  c3.m=3;
  c3.y=2025;
  int n;
  n=3;
  if(n==1){
    printf("%d",c1.a);
  }
  else if(n==2){
    printf("%d",c2.a);
  }
  else{
    printf("%d",c3.a);
  }
}
