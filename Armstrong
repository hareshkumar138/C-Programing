#include <stdio.h>
int main()
{
  int n,rem,a;

  int sum=0;
  scanf("%d",&n);
  a=n;
  while(n!=0)
  { 
    rem=n%10;
    sum=sum+(rem*rem*rem);
    n=n/10;
  }
  if (sum==a)
   { 
     printf("%d is a armstrong number",sum);
   }
 else
  {
  printf("%d is not a armstrong number",sum);
 }
 return 0;
}
